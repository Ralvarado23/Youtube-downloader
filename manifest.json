{
    "manifest_version": 2,
    "name": "Descargador de Youtube",
    "version": "1.0",
    "description": "Una extensi√≥n para descargar el audio y video de Youtube.",
    "permissions": [
        "activeTab",
        "tabs",
        "https://www.y2mate.com/"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "48": "icon.png",
        "128": "icon.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
}